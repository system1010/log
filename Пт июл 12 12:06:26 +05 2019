 #define CREATE_TRACE_POINTS
 #include <trace/events/sched.h>
 
+#include "sched1.h"
+
+
+void  switchtask(void)
+{
+struct task_struct *prev, *next;
+struct rq_flags rf;
+struct rq *rq;
+rq = cpu_rq(smp_processor_id());
+prev = rq->curr;
+
+rq_lock(rq, &rf);
+update_rq_clock(rq);
+next = pick_next_task(rq, prev, &rf);
+if (likely(prev != next)) {
+       rq->curr = next;
+       rq = context_switch(rq, prev, next, &rf);
+}
+rq_unlock_irq(rq, &rf);
+}
+
+
+
+
+
+
